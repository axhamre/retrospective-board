{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "deploy": {
      "cache": false,
      "env": ["AWS_DEFAULT_PROFILE", "AWS_DEFAULT_REGION"],
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "dev": {
    }
  }
}
